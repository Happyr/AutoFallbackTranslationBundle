{
    "name": "happyr/auto-fallback-translation-bundle",
    "type": "symfony-bundle",
    "description": "Make sure to use a translation service like Bing or Google to translate your missing messages",
    "keywords": [],
    "homepage": "http://developer.happyr.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias@happyr.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "symfony/symfony":"^2.7 || ^3.0",
        "php-http/httplug": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/plugins": "^1.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0"
    },
    "suggest": {
        "php-http/httplug-bundle": "To easier configure your httplug clients."
    },
    "autoload": {
        "psr-4": {
            "Happyr\\AutoFallbackTranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Happyr\\AutoFallbackTranslationBundle\\Tests\\": "tests/"
        }
    }
}

